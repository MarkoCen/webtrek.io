{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "editorconfig": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "jsxSingleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "trailingComma": "all",
    "singleAttributePerLine": true,
    "proseWrap": "preserve",
    "quoteProps": "as-needed"
}
